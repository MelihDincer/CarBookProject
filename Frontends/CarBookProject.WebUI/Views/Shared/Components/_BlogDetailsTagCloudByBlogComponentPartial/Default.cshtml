@*Blog Detay Sayfası Main Bölümündeki Tag Kısmı*@

@model List<ResultTagCloudByBlogIdDto>

<div class="tag-widget post-tag-container mb-5 mt-5">
    <div class="tagcloud">
        @foreach(var item in Model)
        {
            <a href="#" class="tag-cloud-link">@item.Name</a>
        }
    </div>
</div>