@*Blog Detay Sayfasında Sağ Kısımda Bulunan Etiket Bulutu Bölümü*@

@model List<ResultTagCloudDto>

<div class="sidebar-box ftco-animate">
    <h3>Etiket Bulutu</h3>
    <div class="tagcloud">
        @foreach(var item in Model)
        {
            <a href="#" class="tag-cloud-link">@item.Name</a>
        }
    </div>
</div>